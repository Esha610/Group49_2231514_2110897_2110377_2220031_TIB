<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Alif.FinancialReportingController">
   <children>
      <Label layoutX="175.0" layoutY="14.0" text="Welcome To TIB" underline="true">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="73.0" text="Financial Reporting" textFill="#051aff">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <ComboBox fx:id="optionsCB" layoutX="45.0" layoutY="124.0" onAction="#ButtonMouseOnClicked" prefWidth="150.0" promptText="Options " />
      <TextArea fx:id="writeReportTA" layoutX="239.0" layoutY="112.0" prefHeight="141.0" prefWidth="200.0" promptText="Write the report on that option chosen." />
      <Button fx:id="createFinancialReport" layoutX="430.0" layoutY="322.0" mnemonicParsing="false" onAction="#createFinancialReportButtonMouseOnClicked" text="Create Financial Report" />
      <Button fx:id="cancel" layoutX="364.0" layoutY="322.0" mnemonicParsing="false" onAction="#cancelButtonMouseOnClicked" text="Cancel" />
   </children>
</AnchorPane>
