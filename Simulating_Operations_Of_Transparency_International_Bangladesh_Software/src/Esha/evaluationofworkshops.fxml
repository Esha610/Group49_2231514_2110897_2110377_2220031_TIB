<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="529.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Esha.EvaluationofworkshopsController">
    <children>
        <BorderPane prefHeight="529.0" prefWidth="769.0">
            <top>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="53.0" prefWidth="771.0" text="Campaign Evaluation Form" textFill="#d70c0c">
               <font>
                  <Font name="System Bold" size="36.0" />
               </font></Label>
            </top>
            <center>
                <VBox alignment="CENTER" spacing="10.0">
                    <Label text="Name" />
                    <TextField fx:id="nameTextField" prefHeight="101.0" prefWidth="769.0" />
                    <Label text="Email" />
                    <TextField fx:id="emailTextField" prefHeight="91.0" prefWidth="769.0" />
                    <Label text="Opinions" />
                    <TextArea fx:id="opinionsTextArea" prefHeight="215.0" prefWidth="769.0" />
                </VBox>
            </center>
            <bottom>
                <HBox alignment="CENTER" prefHeight="98.0" prefWidth="769.0" spacing="20.0">
                    <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitForm" text="Submit" />
                    <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="Back" />
                    <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logOut" text="Log Out" />
                </HBox>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
