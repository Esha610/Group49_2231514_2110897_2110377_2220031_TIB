<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Esha.RequestingMentorshipController">
   <children>
      <Label layoutX="224.0" layoutY="19.0" text="Mentorship Request Form">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="75.0" layoutY="68.0" text="Goals:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="goalsChoiceBox" layoutX="75.0" layoutY="92.0" prefWidth="200.0" />
      <Label layoutX="75.0" layoutY="138.0" text="Skill Development:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="skillChoiceBox" layoutX="75.0" layoutY="162.0" prefWidth="200.0" />
      <Label layoutX="75.0" layoutY="208.0" text="Field of Expertise:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="fieldChoiceBox" layoutX="75.0" layoutY="232.0" prefWidth="200.0" />
      <Label layoutX="75.0" layoutY="278.0" text="Extra:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="extraTextArea" layoutX="75.0" layoutY="302.0" prefHeight="73.0" prefWidth="450.0" promptText="Enter any additional information here..." />
      <Button fx:id="submitButton" layoutX="242.0" layoutY="395.0" mnemonicParsing="false" onAction="#submitForm" text="Submit" />
      <Button fx:id="backButton" layoutX="173.0" layoutY="395.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
      <Button fx:id="logoutButton" layoutX="332.0" layoutY="395.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
   </children>
</AnchorPane>
